  '#ffdb5c',\n            '#ff9f7f',\n            '#fb7293',\n            '#e7bcf3',\n            '#8378ea'\n          ],\n          tooltip: {\n            trigger: 'item',\n            formatter: '{a} <br/>{b} : {c} ({d}%)'\n          },\n          toolbox: {\n            show: true\n          },\n          calculable: true,\n          legend: {\n            orient: 'horizontal',\n            icon: 'circle',\n            bottom: 0,\n            x: 'center',\n            data: val.xData,\n            textStyle: {\n              color: '#fff'\n            }\n          },\n          series: [\n            {\n              name: '通过率统计',\n              type: 'pie',\n              radius: [10, 50],\n              roseType: 'area',\n              center: ['50%', '40%'],\n              itemStyle: {\n                borderRadius: 5\n              },\n              label: {\n                show: true,\n                color: \"#fff\",\n              },\n              emphasis: {\n                label: {\n                  show: false\n                }\n              },\n              data: val.seriesData\n            }\n          ]\n        }\n      },\n      {\n        immediate: true,\n        deep: true\n      }\n    )\n\n    return () => {\n      const height = \"220px\"\n      const width = \"260px\"\n\n      return <div>\n        <echart options={options} height={height} width={width} />\n      </div>\n    }\n  }\n})\n\n"]}]}